            </GridPane>
            <Separator opacity="0.15" prefHeight="3.0" prefWidth="423.0" styleClass="back-white">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Separator>
            <HBox alignment="CENTER" styleClass="back-white">
               <children>
                  <Label text="Ваш статус:">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="status" styleClass="fn-14" text="Label">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="50.0" styleClass="back-white">
               <children>
                  <Button mnemonicParsing="false" onAction="#saveChanges" prefHeight="30.0" prefWidth="100.0" text="Сохранить">
                     <styleClass>
                        <String fx:value="back-transparent" />
                        <String fx:value="back-light-grey" />
                     </styleClass>
                  </Button>
                  <Button fx:id="exit" mnemonicParsing="false" onAction="#exitClicked" prefHeight="30.0" prefWidth="100.0" text="Выйти">
                     <styleClass>
                        <String fx:value="back-transparent" />
                        <String fx:value="back-light-grey" />
                     </styleClass>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="back-light-grey" />
      <String fx:value="back-transparent" />
   </styleClass>
   <padding>
      <Insets left="10.0" />
   </padding>
</VBox>
